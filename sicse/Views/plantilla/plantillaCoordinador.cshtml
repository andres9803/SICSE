                                    <li>@Html.ActionLink("Ascensores", "ascensores", "coordinador", new { area = "" }, null)</li>
                                    <li>@Html.ActionLink("Escaleras", "escaleras", "coordinador", new { area = "" }, null)</li>

                                </ul>
                            </li>
